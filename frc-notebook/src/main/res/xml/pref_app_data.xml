<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:title="@string/pref_header_app_data"
        android:key="pref_app_data" >
        <Preference
            android:key="export_data"
            android:id="@+id/export_data"
            android:title="@string/pref_export_app_data"/>
        <Preference
            android:key="import_data"
            android:id="@+id/import_data"
            android:title="@string/pref_import_app_data"/>
        <Preference
            android:key="data_clear"
            android:id="@+id/data_clear"
            android:title="@string/pref_clear_data"
            android:editable="false"/>
    </PreferenceCategory>
</PreferenceScreen>